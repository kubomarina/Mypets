<h4>ユーザ一覧</h4>
<%= form_with url: search_admin_users_path, local: true, method: :get do |form| %>
    <%= form.text_field :keyword %>
    <%= form.submit "検索" %>
  <% end %>
<table>
 <tr>
  <th>ユーザ名</th>
  <th></th>
  <th>登録日時</th>
  <th>詳細</th>
 </tr>
 <% @users. each do |user| %>
 <tr>
  <td><%= user.owner_name %></td>
  <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
  <td><%= user.created_at.strftime('%Y/%m/%d') %></td>
  <td><%= link_to "→", admin_user_path(user) %></td>
 </tr>
 <% end %>
</table>